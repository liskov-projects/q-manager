{
  "name": "q-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "TAILWIND_CONFIG=tailwind.config.ts next build",
    "start": "next start -p $PORT",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "seed:players": "node src/data/playerSeedingUtility.js",
    "seed:tournaments": "node src/data/tournamentSeedingUtility.js",
    "seed:both": "node src/data/tournamentSeedingUtility.js && node src/data/playerSeedingUtility.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "seed:qa:tournaments": "node src/data/tournamentSeedingQA.js",
    "seed:qa:players": "node src/data/playerSeedingQA.js",
    "seed:qa:both": "npm run seed:qa:tournaments && npm run seed:qa:players"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.3.4",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google-cloud/storage": "^7.16.0",
    "autoprefixer": "^10.4.21",
    "aws-sdk": "^2.1692.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "framer-motion": "^12.6.3",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.2",
    "next": "^14.2.15",
    "papaparse": "^5.5.2",
    "postcss": "^8.5.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "^14.2.0",
    "dependency-cruiser": "^16.10.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5"
  }
}
